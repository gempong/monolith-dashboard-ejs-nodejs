<div class="page-content">
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="/car">Car</a></li>
      <li class="breadcrumb-item active" aria-current="page"><%- title %></li>
    </ol>
  </nav>
  <div class="d-flex w-100 align-items-center justify-content-between mb-4">
    <h1 class="page-title m-0"><%- title %></h1>
    <a class="btn btn-primary" href="/car/create"
      ><i data-feather="plus"></i> Add New Car</a
    >
  </div>
  <div class="d-flex tab-category mb-4">
    <button class="btn btn-outline-primary active">All</button>
    <button class="btn btn-outline-primary">Small</button>
    <button class="btn btn-outline-primary">Medium</button>
    <button class="btn btn-outline-primary">Large</button>
  </div>
  <div class="row">
    <% car.forEach(item => { %>
      <div class="col-md-4">
        <%- include('../../partials/card.ejs', {item}) %>
      </div>
     <% }) %>
  </div>
</div>
